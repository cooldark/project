<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>        
        {{ app('html').title()|raw }}
        {% block javascripts %}
            {%  set asset = app('antares.asset').container('antares.header').style('exception', 'packages/core/css/exception.css')                 
            %} 
            {{ asset.styles()|raw }}
        {% endblock %}
    </head>
    <body>
        {% block content %}
            <div id="app-wrapper">
                <div class="app-content error-page error-page--clean">
                    <section class="main-content">
                    </section>
                </div>
            </div>
        {% endblock %} 
        <div class="modal modal-problem send-notification w700" rel="{{ handles('antares::logger/report',{'csrf':true}) }}" data-title="{{ trans('Send notification to Customer Service') }}" data-error="{{ trans('Unable to load support service.') }}" >
            <div class="col-24 modal-container">

            </div>
        </div>    
    </body>
</html>